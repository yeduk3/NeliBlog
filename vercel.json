{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "/content.js", "use": "@vercel/static" },
    { "src": "/static/js/staticHTMLIncluder.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "/static/js/(.*)", "dest": "/static/js/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
