{
  "version": 2,
  "builds": [
    { "src": "/(.*)", "use": "@vercel/static" },
    { "src": "/static/(.*)", "use": "@vercel/static" },
    { "src": "/js/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" }
  ]
}
